// Use this file as a starting point for your project .eslintrc.
// Copy this file, and add rule overrides as needed.
{
    'extends':'airbnb',
    'rules':{
        'indent':[2],
        'no-underscore-dangle':0,
        'no-console':0,
        'strict':0,
        'new-cap':0,
        'no-caller':0,
        'arrow-body-style':0,
        'no-param-reassign':0,
        'max-len':0,
        'consistent-return':0,
        'global-require':0,
        'no-extend-native':0,
        'prefer-template':0,
        'import/no-unresolved':['error', { ignore:['^components*', '^actions*', '^stores*', '^services*', '^config*', '^scss*'] }],
    }
}
